{
    "rules": {
        "no-console": 0,
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "never"
        ]
    },
    "globals": {
        "Promise": true,
        "phantom": true
    },
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint"
}
